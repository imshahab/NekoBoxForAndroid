        <item name="primaryOrTextSecondary">?colorPrimary</item>
        <item name="primaryOrTextPrimary">?colorPrimary</item>

        <item name="fabColorBackground">?colorPrimaryContainer</item>
        <item name="selectedColorPrimary">?colorPrimary</item>
        <item name="actionBarTheme">@style/ThemeOverlay.Material3.Dark.ActionBar</item>
        <item name="tabTextColor">?colorOnSurfaceVariant</item>
        <item name="tabSelectedTextColor">?colorPrimary</item>
        <item name="tabIndicatorColor">?colorPrimary</item>
        <item name="tabRippleColor">?colorPrimary</item>
        <item name="whiteOrTextPrimary">?colorOnPrimary</item>

        <item name="windowActionModeOverlay">true</item>

        <!-- Remove ActionBar but keep styles and themes -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

        <item name="chipStyle">@style/Widget.Material3.Chip.Assist.SagerNet</item>

